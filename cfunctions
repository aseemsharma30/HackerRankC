#include<stdio.h>
int greatest(int a, int b, int c, int d);
int main()
{
    int a,b,c,d,ans;
    scanf("%d\n%d\n%d\n%d",&a,&b,&c,&d);
    ans=greatest(a,b,c,d);
    printf("%d",ans);
    return 0;

}
int greatest(int a, int b, int c, int d)
{
    if((a>b)&&(a>c)&&(a>d))
    {
        return a;
        //printf("%d",a);
    }
    else if((b>c)&&(b>d))
    {
        return b;
        //printf("%d",b);
    }
    else if((c>d))
    {
        return c;
        //printf("%d",c);
    }
    else 
    {
        return d;
        //printf("%d",d);
    }
}
